<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="add1" class="model.Address">
<property name="line1" value="Borgaon Square"></property>
<property name="line2" value="Gorewada Road"></property>
<property name="city" value="Nagpur"></property>
<property name="pincode" value="440013"></property>
</bean>
      
       <bean id="e1" class="model.Employee" autowire="byType">
   <property name="id" value="101"></property>
   <property name="ename" value="Srushti"></property>
